%Example test case from the task description
sudoku:sudoku({2, [[      [2],      [w],      [e],       []],
                   [       [],  [e,s,4],       [],      [o]],
                   [       [],       [],      [1],       []],
                   [       [],      [w],       [],       []]]}).

%preprocess test
sudoku:sudoku({2, [[      [s],    [w,s],    [w,s],    [w,s]],
                   [      [s],    [w,s],    [w,s],    [w,s]],
                   [      [s],    [w,s],    [w,s],    [w,s]],
                   [       [],      [w],      [w],      [w]]]}).

%corssed out values
sudoku:sudoku({2, [[       [],       [],       [],       []],
                   [       [],      [3],       [],       []],
                   [       [],       [],       [],      [2]],
                   [       [],       [],       [],       []]]}).

%splitting - 4 possile solutions
sudoku:sudoku({2, [[      [1],      [4],       [],       []],
                   [      [2],      [3],       [],       []],
                   [      [3],      [2],       [],       []],
                   [      [4],      [1],       [],       []]]}).

%third test case of the framework
sudoku:sudoku({2, [[     [3],       [w],      [s],       []],
                   [     [e],     [1,w],       [],    [o,w]],
                   [     [s],        [],      [s],    [e,s]],
                   [      [],       [4],      [o],       []]]}).

